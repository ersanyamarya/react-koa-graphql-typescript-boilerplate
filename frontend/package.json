{
  "name": "react-ts-material-parcel-graphql-boilerplate",
  "version": "1.0.0",
  "description": "Boiler plate code for React with TypeScript Material-ui Parcel bundler and Apollo GraphQL",
  "main": "dist/index.html",
  "scripts": {
    "start": "parcel serve 'src/index.html' -p 4200 --open",
    "build": "parcel build './src/index.html' --dist-dir build",
    "prebuild": "npm run clean",
    "clean": "rimraf ./dist ./.parcel-cache",
    "lint-fix": "eslint --ext tsx --fix ./src",
    "lint": "eslint --ext tsx ./src",
    "preformat": "npm run clean",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "codegen": "npx apollo client:codegen --target=typescript --addTypename --endpoint=http://localhost:3000/graphql --outputFlat=src/graphql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ersanyamarya/react-ts-material-parcel-graphql-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ersanyamarya/react-ts-material-parcel-graphql-boilerplate/issues"
  },
  "homepage": "https://github.com/ersanyamarya/react-ts-material-parcel-graphql-boilerplate#readme",
  "dependencies": {
    "@apollo/client": "^3.3.17",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@reach/router": "^1.3.4",
    "date-fns": "^2.21.3",
    "graphql": "^15.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@types/node": "^15.0.3",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  }
}
